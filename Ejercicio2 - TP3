      "source": [
        "import numpy as np\n",
        "from datetime import datetime\n",
        "\n",
        "# --------------------------------------------\n",
        "# Definición de función que transforma el tiempo en  milisegundos \n",
        "tiempo_en_ms = lambda dt:(dt.days * 24 * 60 * 60 + dt.seconds) * 1000 + dt.microseconds / 1000.0\n",
        "# --------------------------------------------\n",
        "\n",
        "a = np.random.randn(30,100).astype(dtype=np.float32)\n",
        "b = np.random.randn(100,25).astype(dtype=np.float32)\n",
        "# print(b)\n",
        "\n",
        "tiempo_CPU = datetime.now()\n",
        "res = a.dot(b)\n",
        "tiempo_CPU = datetime.now() - tiempo_CPU\n",
        "\n",
        "c = np.zeros_like(res)\n",
        "# print(c)\n",
        "\n",
        "a_gpu = cuda.mem_alloc(a.nbytes)\n",
        "cuda.memcpy_htod(a_gpu,a)\n",
        "\n",
        "b_gpu = cuda.mem_alloc(b.nbytes)\n",
        "cuda.memcpy_htod(b_gpu,b)\n",
        "\n",
        "c_gpu = cuda.mem_alloc(c.nbytes)\n",
        "cuda.memcpy_htod(c_gpu,c)\n",
        "\n",
        "\n",
        "func = mod.get_function(\"multiplyMatrixOneBlock\")\n",
        "tiempo_GPU = datetime.now()\n",
        "func(a_gpu,b_gpu,c_gpu,np.int32(a.shape[1]),np.int32(b.shape[1]),block=(np.int(b.shape[1]),np.int(a.shape[0]),1),grid=(1,1,1))\n",
        "tiempo_GPU = datetime.now() - tiempo_GPU\n",
        "\n",
        "cuda.memcpy_dtoh(c,c_gpu)\n",
        "\n",
        "print(res)\n",
        "print(\"\\n\")\n",
        "print(c)\n",
        "\n",
        "print(\"Resultado: \",(c==res).all())\n",
        "print(\"Tiempo CPU: \",tiempo_en_ms( tiempo_CPU ), \"[ms]\" )\n",
        "print(\"Tiempo GPU: \",tiempo_en_ms( tiempo_GPU ), \"[ms]\" )"
